<?xml version="1.0"?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <argument name="data" xsi:type="array">
        <item name="js_config" xsi:type="array">
            <item name="provider" xsi:type="string">
                check24_orderimport_listing.check24_orderimport_listing_data_source
            </item>
            <item name="deps" xsi:type="string">check24_orderimport_listing.check24_orderimport_listing_data_source
            </item>
        </item>
        <item name="spinner" xsi:type="string">check24_orderimport_columns</item>
        <item name="buttons" xsi:type="array">
            <item name="import" xsi:type="array">
                <item name="name" xsi:type="string">import</item>
                <item name="label" xsi:type="string" translate="true">manual import</item>
                <item name="class" xsi:type="string">primary</item>
                <item name="url" xsi:type="string">*/*/import</item>
            </item>
            <item name="process" xsi:type="array">
                <item name="name" xsi:type="string">process</item>
                <item name="label" xsi:type="string" translate="true">process orders</item>
                <item name="class" xsi:type="string">primary</item>
                <item name="url" xsi:type="string">*/*/process</item>
            </item>
            <item name="response" xsi:type="array">
                <item name="name" xsi:type="string">response</item>
                <item name="label" xsi:type="string" translate="true">response orders</item>
                <item name="class" xsi:type="string">primary</item>
                <item name="url" xsi:type="string">*/*/response</item>
            </item>
            <item name="dispatch" xsi:type="array">
                <item name="name" xsi:type="string">dispatch</item>
                <item name="label" xsi:type="string" translate="true">send dispatch</item>
                <item name="class" xsi:type="string">primary</item>
                <item name="url" xsi:type="string">*/*/dispatch</item>
            </item>
            <item name="process cancel" xsi:type="array">
                <item name="name" xsi:type="string">response</item>
                <item name="label" xsi:type="string" translate="true">process cancel</item>
                <item name="class" xsi:type="string">primary</item>
                <item name="url" xsi:type="string">*/*/processcancel</item>
            </item>
            <item name="cancel response" xsi:type="array">
                <item name="name" xsi:type="string">response</item>
                <item name="label" xsi:type="string" translate="true">cancel response</item>
                <item name="class" xsi:type="string">primary</item>
                <item name="url" xsi:type="string">*/*/cancelresponse</item>
            </item>
            <item name="process return" xsi:type="array">
                <item name="name" xsi:type="string">response</item>
                <item name="label" xsi:type="string" translate="true">process return</item>
                <item name="class" xsi:type="string">primary</item>
                <item name="url" xsi:type="string">*/*/processreturn</item>
            </item>
        </item>
    </argument>
    <dataSource name="check24_orderimport_listing_data_source">
        <argument name="dataProvider" xsi:type="configurableObject">
            <argument name="class" xsi:type="string">Check24\OrderImport\Ui\Component\Listing\DataProvider\Order
            </argument>
            <argument name="name" xsi:type="string">check24_orderimport_listing_data_source</argument>
            <argument name="primaryFieldName" xsi:type="string">id</argument>
            <argument name="requestFieldName" xsi:type="string">id</argument>
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="update_url" xsi:type="url" path="mui/index/render"/>
                    <item name="storageConfig" xsi:type="array">
                        <item name="indexField" xsi:type="string">id</item>
                    </item>
                </item>
            </argument>
        </argument>
        <argument name="data" xsi:type="array">
            <item name="js_config" xsi:type="array">
                <item name="component" xsi:type="string">Magento_Ui/js/grid/provider</item>
            </item>
        </argument>
    </dataSource>

    <container name="listing_top">
        <argument name="data" xsi:type="array">
            <item name="config" xsi:type="array">
                <item name="template" xsi:type="string">ui/grid/toolbar</item>
                <item name="stickyTmpl" xsi:type="string">ui/grid/sticky/toolbar</item>
            </item>
        </argument>
        <bookmark name="bookmarks">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="storageConfig" xsi:type="array">
                        <item name="namespace" xsi:type="string">check24_orderimport_order_listing</item>
                    </item>
                </item>
            </argument>
        </bookmark>
        <component name="columns_controls">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="columnsData" xsi:type="array">
                        <item name="provider" xsi:type="string">
                            check24_orderimport_listing.check24_orderimport_listing.check24_orderimport_columns
                        </item>
                    </item>
                    <item name="component" xsi:type="string">Magento_Ui/js/grid/controls/columns</item>
                    <item name="displayArea" xsi:type="string">dataGridActions</item>
                </item>
            </argument>
        </component>
        <filterSearch name="fulltext"/>
        <filters name="listing_filters"/>
        <!--<massaction name="listing_massaction">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="selectProvider" xsi:type="string">check24_orderimport_order_grid.check24_orderimport_order_grid.check24_order_columns.ids</item>
                    <item name="indexField" xsi:type="string">id</item>
                </item>
            </argument>
            <action name="retry">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="type" xsi:type="string">retry</item>
                        <item name="label" xsi:type="string" translate="true">Retry Processing</item>
                        <item name="url" xsi:type="url" path="check24_orderimport/order/retry"/>
                    </item>
                </argument>
            </action>
        </massaction>-->
        <paging name="listing_paging">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="storageConfig" xsi:type="array">
                        <item name="provider" xsi:type="string">
                            check24_orderimport_listing.check24_orderimport_listing.listing_top.bookmarks
                        </item>
                        <item name="namespace" xsi:type="string">current.paging</item>
                    </item>
                    <item name="selectProvider" xsi:type="string">
                        check24_orderimport_listing.check24_orderimport_listing.check24_orderimport_columns.ids
                    </item>
                </item>
            </argument>
        </paging>
    </container>

    <columns name="check24_orderimport_columns">
        <argument name="data" xsi:type="array">
            <item name="config" xsi:type="array">
                <item name="storageConfig" xsi:type="array">
                    <item name="provider" xsi:type="string">
                        check24_orderimport_listing.check24_orderimport_listing.listing_top.bookmarks
                    </item>
                    <item name="namespace" xsi:type="string">current</item>
                </item>
                <item name="childDefaults" xsi:type="array">
                    <item name="storageConfig" xsi:type="array">
                        <item name="provider" xsi:type="string">
                            check24_orderimport_listing.check24_orderimport_listing.listing_top.bookmarks
                        </item>
                        <item name="root" xsi:type="string">columns.${ $.index }</item>
                        <item name="namespace" xsi:type="string">current.${ $.storageConfig.root}</item>
                    </item>
                </item>
            </item>
        </argument>
        <selectionsColumn name="ids">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="resizeEnabled" xsi:type="boolean">false</item>
                    <item name="resizeDefaultWidth" xsi:type="string">55</item>
                    <item name="indexField" xsi:type="string">id</item>
                </item>
            </argument>
        </selectionsColumn>
        <column name="id">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">textRange</item>
                    <item name="sorting" xsi:type="string">desc</item>
                    <item name="label" xsi:type="string" translate="true">ID</item>
                </item>
            </argument>
        </column>
        <column name="check24_order_id" class="Check24\OrderImport\Ui\Component\Listing\Column\Order\Check24OrderId">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="bodyTmpl" xsi:type="string">ui/grid/cells/html</item>
                    <item name="label" xsi:type="string" translate="true">CHECK24 Order ID</item>
                </item>
            </argument>
        </column>
        <column name="status">
            <argument name="data" xsi:type="array">
                <item name="options" xsi:type="object">
                    Check24\OrderImport\Ui\Component\Listing\Column\Order\Status\Options
                </item>
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">select</item>
                    <item name="component" xsi:type="string">Magento_Ui/js/grid/columns/select</item>
                    <item name="editor" xsi:type="string">select</item>
                    <item name="dataType" xsi:type="string">select</item>
                    <item name="label" xsi:type="string" translate="true">Status</item>
                </item>
            </argument>
        </column>
        <column name="magento_order_increment_id"
                class="Check24\OrderImport\Ui\Component\Listing\Column\Order\MagentoOrderIncrementId">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="bodyTmpl" xsi:type="string">ui/grid/cells/html</item>
                    <item name="label" xsi:type="string" translate="true">Magento Order ID</item>
                </item>
            </argument>
        </column>
        <column name="error" class="Check24\OrderImport\Ui\Component\Listing\Column\Order\Error">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">text</item>
                    <item name="bodyTmpl" xsi:type="string">ui/grid/cells/html</item>
                    <item name="label" xsi:type="string" translate="true">Error</item>
                </item>
            </argument>
        </column>
        <column name="type">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="bodyTmpl" xsi:type="string">ui/grid/cells/html</item>
                    <item name="label" xsi:type="string" translate="true">Type</item>
                </item>
            </argument>
        </column>
        <column name="created_at" class="Magento\Ui\Component\Listing\Columns\Date">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">dateRange</item>
                    <item name="component" xsi:type="string">Magento_Ui/js/grid/columns/date</item>
                    <item name="dataType" xsi:type="string">date</item>
                    <item name="label" xsi:type="string" translate="true">Created At</item>
                </item>
            </argument>
        </column>
        <column name="action" class="Check24\OrderImport\Ui\Component\Listing\Column\Order\PageActions">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="bodyTmpl" xsi:type="string">ui/grid/cells/html</item>
                    <item name="label" xsi:type="string" translate="true">Action</item>
                </item>
            </argument>
        </column>
    </columns>
</listing>
